<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.rupa958823" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Rupa</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://ionic.local/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/android_img/icon/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="www/android_img/icon/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="www/android_img/icon/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="www/android_img/icon/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="www/android_img/icon/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="www/android_img/icon/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="www/android_img/splash/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="www/android_img/splash/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="www/android_img/splash/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="www/android_img/splash/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="www/android_img/splash/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="www/android_img/splash/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="www/android_img/splash/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="www/android_img/splash/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="www/android_img/splash/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="www/android_img/splash/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="www/android_img/splash/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="www/android_img/splash/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="www/ios_img/icon/icon.png" width="57" />
        <icon height="114" src="www/ios_img/icon/icon@2x.png" width="114" />
        <icon height="40" src="www/ios_img/icon/icon-40.png" width="40" />
